                            <div class="dropdown-menu box-shadow-v2 dropdown__col-2" aria-labelledby="navbarDropdown">
                                <a class="dropdown-item" href="@Url.Action("about", "Pages")">About</a>
                                <a class="dropdown-item" href="@Url.Action("portfolio", "Pages")">Portfolio</a>
                                <a class="dropdown-item" href="@Url.Action("portfolio_details", "Pages")">Portfolio Details</a>
