                        <ul class="list-unstyled">
                            <li class="mb-2">
                                <span class="icon icon-Phone2 text-primary mr-2"></span> 123. 456. 789
                            </li>
                            <li class="mb-2">
                                <span class="icon icon-Mail text-primary mr-2"></span>
                                <a href="blog.devdap@gmail.com">blog.devdap@gmail.com</a>
                            </li>
                            <li class="mb-2">
                                <span class="icon icon-Pointer text-primary mr-2"></span> Punjab, Pakistan
                            </li>
                        </ul>
                        <ul class="list-inline social social-rounded social-white mt-4">
                            <li class="list-inline-item">
                                <a href="#">
                                    <i class="fa fa-facebook"></i>
                                </a>
                            </li>
                            <li class="list-inline-item">
                                <a href="#">
                                    <i class="fa fa-twitter"></i>
                                </a>
                            </li>
                            <li class="list-inline-item">
                                <a href="#">
                                    <i class="fa fa-google-plus"></i>
                                </a>
                            </li>
                            <li class="list-inline-item">
                                <a href="#">
                                    <i class="fa fa-linkedin"></i>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
